{
  // 编辑器设置
  "editor.formatOnSave": false, // 保存时格式化代码
  "editor.unicodeHighlight.nonBasicASCII": false, // 不高亮非基本ASCII字符
  "editor.minimap.enabled": false, // 不显示缩略图
  "editor.snippetSuggestions": "top", // 代码片段建议置顶
  "editor.cursorSmoothCaretAnimation": "on", // 光标平滑动画
  "editor.guides.bracketPairs": "active", // 活跃的括号匹配显示
  "editor.lineNumbers": "interval", // 间隔显示行号
  "editor.fontFamily": "Input, monospace", // 字体设置为 Input Mono, monospace
  "editor.renderWhitespace": "boundary", // 显示空白字符边界
  "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?", // 单词分隔符设置
  "editor.find.addExtraSpaceOnTop": false, // 查找不在顶部显示额外空间
  "editor.inlineSuggest.enabled": true, // 启用内联建议
  "editor.multiCursorModifier": "ctrlCmd", // 多光标修改键
  "editor.suggestSelection": "first", // 选择第一个建议
  "editor.tabSize": 2, // 制表符大小为 2
  "editor.unicodeHighlight.invisibleCharacters": false, // 不高亮不可见字符
  "editor.stickyScroll.enabled": true, // 启用粘性滚动
  "editor.hover.sticky": true, // 悬停信息保持固定
  // 编辑器在保存时执行的代码操作设置
  "editor.codeActionsOnSave": {
      // "source.fixAll": "never", // 不自动修复所有问题
      "source.fixAll.eslint": "explicit", // 显式ESLint修复
      "source.organizeImports": "never" // 不自动整理导入项
  },
  // 调试
  "debug.onTaskErrors": "debugAnyway", // 无论如何调试任务错误
  "diffEditor.ignoreTrimWhitespace": false, // 不忽略空白的差异编辑器
  "git.autofetch": true, // 自动获取Git
  "git.enableSmartCommit": true, // 启用智能提交功能
  "git.untrackedChanges": "separate", // 显示未跟踪的更改为独立部分
  "scm.diffDecorationsGutterWidth": 2, // 差异修饰符边栏宽度设置为2
  // 扩展
  "extensions.autoUpdate": "onlyEnabledExtensions", // 仅自动更新已启用的扩展
  "emmet.showSuggestionsAsSnippets": true, // 将Emmet建议显示为代码片段
  "emmet.triggerExpansionOnTab": false, // 禁用Tab键触发Emmet展开
  "errorLens.enabledDiagnosticLevels": [
      "warning", // 启用警告级别显示
      "error" // 启用错误级别显示
  ],
  "errorLens.excludeBySource": [
      "cSpell", // 不在编辑器中显示cSpell源的错误信息
      "Grammarly", // 不在编辑器中显示Grammarly源的错误信息
      "eslint" // 不在编辑器中显示ESLint源的错误信息
  ],
  // 文件和资源管理器设置
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "files.autoSave": "onFocusChange",
  "explorer.confirmDelete": false, // 删除文件时不提示确认
  "explorer.compactFolders": false, // 紧凑文件树结构
  "explorer.confirmDragAndDrop": false, // 拖放文件时不提示确认
  // 工作台设置
  "workbench.colorTheme": "Vitesse Dark", // 配色主题
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right", // 侧栏位置在右边
  "workbench.settings.editor": "json", // 默认使用 JSON 格式的设置编辑器
  "workbench.editor.empty.hint": "hidden", // 控制工作台空提示的显示
  "workbench.startupEditor": "none", // 启动时不显示欢迎页面
  "workbench.iconTheme": "catppuccin-macchiato", // 图标主题
  "workbench.productIconTheme": "icons-carbon", // 产品图标主题
  "workbench.list.smoothScrolling": true, // 平滑滚动
  "workbench.tree.expandMode": "singleClick", // 单击展开树状视图
  "workbench.tree.indent": 10, // 树状视图缩进
  "window.dialogStyle": "custom", // 对话框样式为自定义
  "window.title": "02✧( •̀ ω •́ )✧${rootName}✧", // 窗口标题文本
  "window.titleBarStyle": "custom", // 标题栏样式为自定义
  "window.menuBarVisibility": "compact", // 菜单栏紧凑模式
  "window.commandCenter": false, // 命令中心不显示
  "workbench.editor.tabActionLocation": "left", // 选项卡动作位置在左侧
  "workbench.editor.closeOnFileDelete": true, // 删除文件时关闭对应编辑器
  "workbench.editor.highlightModifiedTabs": true, // 高亮修改过的标签页
  "workbench.editor.limit.enabled": true, // 启用编辑器数量限制
  "workbench.editor.limit.perEditorGroup": true, // 每个编辑器组限制
  "workbench.editor.limit.value": 5, // 编辑器数量限制为 5
  // 终端设置
  "terminal.integrated.cursorBlinking": true, // 光标闪烁
  "terminal.integrated.cursorStyle": "line", // 光标样式为线型
  "terminal.integrated.fontWeight": "300", // 终端字体粗细
  "terminal.integrated.persistentSessionReviveProcess": "never", // 终端会话不自动恢复
  "terminal.integrated.tabs.enabled": true, // 启用终端选项卡
  //search
  "search.exclude": {
      "**/*.snap": true,
      "**/*.svg": true,
      "**/.git": true,
      "**/.github": false,
      "**/.nuxt": true,
      "**/.output": true,
      "**/.pnpm": true,
      "**/.vscode": true,
      "**/.yarn": true,
      "**/assets": true,
      "**/bower_components": true,
      "**/dist/**": true,
      "**/logs": true,
      "**/node_modules": true,
      "**/out/**": true,
      "**/package-lock.json": true,
      "**/pnpm-lock.yaml": true,
      "**/public": true,
      "**/temp": true,
      "**/yarn.lock": true,
      "**/CHANGELOG*": true,
      "**/LICENSE*": true
  },
  // 安全设置
  "security.workspace.trust.untrustedFiles": "open",
  // 版本控制设置
  "git.confirmSync": false,
  // 代码管理和其他工具
  "javascript.updateImportsOnFileMove.enabled": "always", // 启用移动文件时自动更新导入路径的功能
  "Codegeex.Privacy": true,
  "markdown-preview-enhanced.revealjsTheme": "black.css", // Markdown 预览主题
  // 语言特定设置
  "[markdown]": {
      "editor.wordBasedSuggestions": "off",
      "editor.parameterHints.enabled": true,
      "editor.quickSuggestionsDelay": 10,
      "editor.quickSuggestions": {
          "other": true,
          "comments": false,
          "strings": false
      }
  },
  //eslint
  "eslint.codeAction.showDocumentation": { // 控制是否显示 eslint 规则的文档
      "enable": true // 启用
  },
  "eslint.quiet": true, // 静默模式，只显示错误，忽略警告
  "eslint.rules.customizations": [ // 自定义 eslint 规则
      {
          "rule": "style/*", // 样式类规则
          "severity": "off", // 关闭此类规则
          "fixable":true,
      },
      {
          "rule": "format/*", // 格式化类规则
          "severity": "off", // 关闭此类规则
          "fixable":true,
      },
      {
          "rule": "*-indent", // 缩进相关规则
          "severity": "off", // 关闭缩进相关规则
          "fixable":true,
      },
      {
          "rule": "*-spacing", // 空格相关规则
          "severity": "off", // 关闭空格相关规则
          "fixable":true,
      },
      {
          "rule": "*-spaces", // 空格使用规则
          "severity": "off", // 关闭空格使用规则
          "fixable":true,
      },
      {
          "rule": "*-order", // 顺序规则
          "severity": "off", // 关闭顺序规则
          "fixable":true,
      },
      {
          "rule": "*-dangle", // 尾随逗号规则
          "severity": "off", // 关闭尾随逗号规则
          "fixable":true,
      },
      {
          "rule": "*-newline", // 换行规则
          "severity": "off", // 关闭换行规则
          "fixable":true,
      },
      {
          "rule": "*quotes", // 引号规则
          "severity": "off", // 关闭引号规则
          "fixable":true,
      },
      {
          "rule": "*semi", // 分号规则
          "severity": "off", // 关闭分号规则
          "fixable":true,
      }
  ],
  "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "vue",
      "html",
      "markdown",
      "json",
      "jsonc",
      "yaml",
      "toml",
      "xml",
      "gql",
      "graphql",
      "astro",
      "svelte",
      "css",
      "less",
      "scss",
      "pcss",
      "postcss"
  ],
  "gitlens.menus": {
      "editor": {
          "blame": false,
          "clipboard": true,
          "compare": true,
          "history": false,
          "remote": false
      },
      "editorGroup": {
          "blame": true,
          "compare": false
      },
      "editorTab": {
          "clipboard": true,
          "compare": true,
          "history": true,
          "remote": true
      },
      "explorer": {
          "clipboard": true,
          "compare": true,
          "history": true,
          "remote": true
      },
      "scm": {
          "authors": true
      },
      "scmGroup": {
          "compare": true,
          "openClose": true,
          "stash": true
      },
      "scmGroupInline": {
          "stash": true
      },
      "scmItem": {
          "clipboard": true,
          "compare": true,
          "history": true,
          "remote": false,
          "stash": true
      }
  },
